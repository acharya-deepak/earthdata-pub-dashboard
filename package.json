{
  "name": "earthdata-pub-dashboard",
  "version": "0.1.0",
  "description": "A dashboard for Earthdata Pub API",
  "repository": {
    "type": "git",
    "url": "https://git.earthdata.nasa.gov/scm/EDPUB/dashboard.git"
  },
  "author": {
    "name": "Earthdata Pub Dev Team"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://bugs.earthdata.nasa.gov/browse/EDPUB"
  },
  "homepage": "https://git.earthdata.nasa.gov/projects/EDPUB/repos/dashboard/browse",
  "private": true,
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "audit-ci": "audit-ci --high -p npm",
    "serve": "NODE_ENV=development webpack serve --config webpack.dev.js",
    "serve-hot": "NODE_ENV=development webpack serve --hot --config webpack.dev.js",
    "serve-cypress": "SHOW_DISTRIBUTION_API_METRICS=true ESROOT=http://example.com npm run serve",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "watch": "webpack --watch",
    "lint": "eslint app/src/js/ --ext .js",
    "lint-fix": "eslint --fix app/src/js/ --ext .js",
    "cypress": "LOCALSTACK_HOST=localhost LOCAL_ES_HOST=localhost CYPRESS_TESTING=true NODE_ENV=test cypress open",
    "cypress-ci": "LOCALSTACK_HOST=localhost LOCAL_ES_HOST=localhost CYPRESS_TESTING=true NODE_ENV=test cypress run --spec 'cypress/integration/*' --config video=true",
    "validate": "ava cypress/validation-tests/*.js",
    "test": "NODE_ENV=test npm run lint && ava",
    "e2e-tests": "npm run start-api; docker-compose -f ./localAPI/docker-compose-dashboard.yml -f ./localAPI/docker-compose-cypress.yml up --exit-code-from=e2e --abort-on-container-exit",
    "validation-tests": "npm run start-api; docker-compose -f ./localAPI/docker-compose-dashboard.yml -f ./localAPI/docker-compose-validate.yml up --exit-code-from=validate --abort-on-container-exit",
    "link-api": "npm link ../api",
    "start-api": "npm run link-api; docker-compose -f ./node_modules/earthdata-pub-modules/docker-compose.yml up -d; ",
    "stop-api": "docker-compose -f ./node_modules/earthdata-pub-modules/docker-compose.yml down",
    "start-dashboard": "docker-compose -f ./localAPI/docker-compose-dashboard.yml up -d",
    "stop-dashboard": "docker-compose -f ./localAPI/docker-compose-dashboard.yml down",
    "start": "npm run start-api; npm run start-dashboard",
    "stop": "npm run stop-api; npm run stop-dashboard",
    "view-logs": "npm run start-api; docker-compose -f ./localAPI/docker-compose-dashboard.yml up"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx",
          [
            "babel-plugin-webpack-alias-7",
            {
              "config": "./config/webpack.config.test.js"
            }
          ]
        ]
      },
      "extensions": [
        "js",
        "jsx",
        "ts"
      ]
    },
    "require": [
      "@babel/register",
      "./test/_setup-browser-env.js",
      "ignore-styles"
    ],
    "files": [
      "test/**/*",
      "!node_modules/",
      "!test/fixtures/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 safari versions"
    ]
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@cypress/webpack-preprocessor": "^4.1.2",
    "@types/retry": "0.12.0",
    "@webpack-cli/serve": "^1.0.1",
    "ajv": "^6.9.1",
    "audit-ci": "^3.1.0",
    "autoprefixer": "^9.8.6",
    "ava": "^3.13.0",
    "ava-http": "^1.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-webpack-alias-7": "^0.1.1",
    "browser-env": "^3.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cls-bluebird": "^2.1.0",
